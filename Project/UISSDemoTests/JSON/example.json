{
    "Variables": {
        "color": "yellowColor"
    },
    "UIToolbar": {
        "tintColor": "$color",
        "backgroundImage:any:default": "black"
    },
    "UINavigationBar": {
        "Pad": {
            "tintColor": "green"
        },
        "Phone": {
            "tintColor": "red"
        },
        "titleVerticalPositionAdjustment:default": 10,
        "backgroundImage:landscapePhone": "black"
    },
    "UITabBarItem": {
        "titlePositionAdjustment": [10, 10],
        "titleTextAttributes:normal": {"font":24}
    },
    "UILabel#red":{
        "textAttributes":{
            "textColor":"red"
        }
    },
    "UILabel#magenta":{
        "textAttributes":{
            "textColor":"magenta"
        }
    },
    "UILabel#overridden":{
        "textAttributes":{
            "textColor":"yellow"
        }
    },
    "UIToolbar#magenta": {
        "barTintColor": "magenta",
        "tintColor": "white",
        "UILabel":{
            "textAttributes":{
                "textColor":"cyan"
            }
        },
        "UILabel#overridden":{
            "textAttributes":{
                "textColor":"purple"
            }
        }
    }
}
